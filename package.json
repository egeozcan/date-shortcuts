{
  "name": "date-shortcut-parser",
  "version": "1.0.2",
  "description": "A lightweight and powerful TypeScript library for parsing human-readable date shortcuts into `Date` objects.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/egeozcan/date-shortcuts#readme",
  "bugs": {
    "url": "https://github.com/egeozcan/date-shortcuts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egeozcan/date-shortcuts.git"
  },
  "author": "Yavuz Ege Ã–zcan",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "keywords": [
    "date",
    "time",
    "parser",
    "shortcut",
    "datetime",
    "natural language",
    "date-parsing",
    "typescript"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && npm publish"
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
    "bumpp": "^10.1.0",
    "tsdown": "^0.11.9",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  }
}
